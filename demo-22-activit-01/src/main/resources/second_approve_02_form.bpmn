        <omgdi:waypoint x="410.0" y="257.0"></omgdi:waypoint>
        <omgdi:waypoint x="510.0" y="257.0"></omgdi:waypoint>
      </bpmndi:BPMNEdge>
      <bpmndi:BPMNEdge bpmnElement="flow10" id="BPMNEdge_flow10">
