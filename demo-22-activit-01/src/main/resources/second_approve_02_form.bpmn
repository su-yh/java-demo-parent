        <omgdi:waypoint x="583.0" y="70.0"></omgdi:waypoint>
        <omgdi:waypoint x="292.0" y="70.0"></omgdi:waypoint>
        <omgdi:waypoint x="292.0" y="150.0"></omgdi:waypoint>
